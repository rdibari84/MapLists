{
  "manifest_version": 2,
  "name": "GoogleMaps List Extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.google.de/maps/@*/data=*",
        "https://www.google.com/maps/@*/data=*"
      ],
      "js": ["jquery-3.3.1.min.js", "content.js"],
      "css":["bootstrap-4.0.0/scss/bootstrap.scss"],
      "all_frames" : false,
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "peristence": true
  },
  "permissions": [
    "storage", "tabs", "activeTab",  "history", "webNavigation", "https://www.google.de/maps/@*/data=*"
  ]
}
