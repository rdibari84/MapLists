{
  "manifest_version": 2,
  "name": "GoogleMaps List Extension",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "https://www.google.de/maps/*",
        "https://www.google.com/maps/*"
      ],
      "js": ["content.js"],
      "all_frames" : false,
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "peristence": true
  },
  "web_accessible_resources":[
    "googlemapscode.js",
  ],
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "history",
    "https://www.google.de/maps/*"
  ]
}
